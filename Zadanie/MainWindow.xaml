                        <Button Content="Wybierz zdjęcie" Width="100" Click="Zdjecie_Click"/>
                        <TextBox Visibility="Hidden" IsEnabled="False"></TextBox>
                        <Image Name="zdjecie_u" Width="25px" Height="25px"></Image>
                    </StackPanel>
                    
